{
  "trigger": "webhook.received",
  "actions": [
    {
      "action": "openai.generate_text",
      "params": {
        "prompt": "Summarize the following data: {{$json.webhookData}}",
        "model": "gpt-3.5-turbo",
        "temperature": 0.7,
        "maxTokens": 150
      },
      "mode": "sequential"
    },
    {
      "action": "gmail.send_email",
      "params": {
        "to": "user@example.com",
        "subject": "Data Summary",
        "message": "Summary: {{$node[\"OpenAI\"].json.output}}"
      },
      "mode": "sequential"
    }
  ]
}
